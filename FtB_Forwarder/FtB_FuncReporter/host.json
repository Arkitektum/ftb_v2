{
  "version": "2.0",
  "logging": {
    "includeScopes": true,
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "default": "Debug",
      "Microsoft": "Error"
    }
  },
  "extensions": {
    "serviceBus": {
      "prefetchCount": 20,
      "messageHandlerOptions": { "maxConcurrentCalls": 20 },
      "sessionHandlerOptions": { "maxConcurrentCalls": 20 }
    }
  }
}