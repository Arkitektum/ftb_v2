{
  "version": "2.0",
  "logging": {
    "includeScopes": true,
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "default": "Debug",
      "Microsoft": "Error"
    }
  },
  "extensions": {
    "serviceBus": {
      "prefetchCount": 1,
      "messageHandlerOptions": {
        "maxConcurrentCalls": 1,
        "maxAutoRenewDuration": "00:10:00"
      },
      "sessionHandlerOptions": { "maxConcurrentCalls": 1   }
      }
    }
  }